    <schedule schedule-id="ExportCatalog">
        <description>Export complete categories of the catalog</description>
        <enabled-flag>false</enabled-flag>
        <execution-scope>site</execution-scope>
        <pipeline>
            <name>Custom_FeedJobs</name>
            <startnode>StartExportCatalog</startnode>
        </pipeline>
        <sites>
            <site site-id="SiteGenesis"/>
        </sites>
        <run-once>2013-01-27T23:23:00.000Z</run-once>
        <notification>
            <enabled-flag>true</enabled-flag>
            <email-from>donotreply@Sites.com</email-from>
            <email-to>bsudarsan@mainstreetcommerce.com</email-to>
            <threshold>
                <success/>
                <error/>
                <hang/>
                <retry/>
            </threshold>
            <max-execution-time>60</max-execution-time>
            <hang-detection-enabled-flag>false</hang-detection-enabled-flag>
            <failure-rule>continue</failure-rule>
        </notification>
    </schedule>

