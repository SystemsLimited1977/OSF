<?xml version="1.0" encoding="UTF-8"?>
<schedules xmlns="http://www.demandware.com/xml/impex/schedules/2007-03-31">
    <schedule schedule-id="ltkProcessing">
        <description>Listrak Data Processing</description>
        <enabled-flag>false</enabled-flag>
        <execution-scope>site</execution-scope>
        <pipeline>
            <name>ltkDataSync</name>
            <startnode>Processing</startnode>
        </pipeline>
        <sites>
            <site site-id="SiteGenesis"/>
        </sites>
        <recurrence>
            <date-from>2012-04-02Z</date-from>
            <start-time>04:00:00.000Z</start-time>
            <interval>10m</interval>
            <day-of-week>
                <weekday>Sunday</weekday>
                <weekday>Monday</weekday>
                <weekday>Tuesday</weekday>
                <weekday>Wednesday</weekday>
                <weekday>Thursday</weekday>
                <weekday>Friday</weekday>
                <weekday>Saturday</weekday>
            </day-of-week>
        </recurrence>
        <notification>
            <enabled-flag>false</enabled-flag>
            <hang-detection-enabled-flag>false</hang-detection-enabled-flag>
        </notification>
    </schedule>

    <schedule schedule-id="ltkProcessing-Daily">
        <description>Listrak Daily Processing</description>
        <enabled-flag>false</enabled-flag>
        <execution-scope>site</execution-scope>
        <pipeline>
            <name>ltkDataSync</name>
            <startnode>ProcessingDaily</startnode>
        </pipeline>
        <sites>
            <site site-id="SiteGenesis"/>
        </sites>
        <recurrence>
            <date-from>2012-04-02Z</date-from>
            <start-time>06:00:00.000Z</start-time>
            <interval>1d</interval>
            <day-of-week>
                <weekday>Sunday</weekday>
                <weekday>Monday</weekday>
                <weekday>Tuesday</weekday>
                <weekday>Wednesday</weekday>
                <weekday>Thursday</weekday>
                <weekday>Friday</weekday>
                <weekday>Saturday</weekday>
            </day-of-week>
        </recurrence>
        <notification>
            <enabled-flag>false</enabled-flag>
            <hang-detection-enabled-flag>false</hang-detection-enabled-flag>
        </notification>
    </schedule>

</schedules>
